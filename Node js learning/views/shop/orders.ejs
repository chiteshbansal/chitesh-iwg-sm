
<%- include('../includes/head.ejs')%>
</head>
<body>
  <%- include('../includes/navigation.ejs') %>
  <main>
    <% if(orders.length<=0){%>
      <h1>You haven't Ordered anything yet!
      </h1>
      <%}else{%>
        <ul>
        <% orders.forEach(order=>{%>
            <li>
                <h1><%=order.id%></h1>
                <ul>
                    <% order.products.forEach(product=>{%>
                        <li>
                          <h3><%=product.title%><span>[<%=product.orderItem.quantity%>]</span></h3>
                          
                        </li>
                   <% })%>
                </ul>
            </li>
        <%})%>
        </ul>
      <%}%>
  </main>
  <%- include('../includes/end.ejs') %>  